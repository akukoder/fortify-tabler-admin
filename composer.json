{
    "name": "akukoder/fortify-soft-ui",
    "description": "Laravel Fortify based on FortifyUI and built using the open source Tabler theme.",
    "keywords": [
      "laravel",
      "fortify",
      "fortify-ui",
      "fortify-tabler-admin",
      "tabler"
    ],
    "homepage": "https://github.com/akukoder/fortify-tabler-admin",
    "license": "MIT",
    "authors": [
      {
        "name": "Syahril Zulkefli",
        "email": "syahzul@akukoder.com",
        "homepage": "https://akukoder.com",
        "role": "Developer"
      }
    ],
    "require": {
      "illuminate/support": "^8.0",
      "zacksmash/fortify-ui": "^1.0"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^2.16",
      "orchestra/testbench": "^6.0",
      "phpunit/phpunit": "^9.3",
      "vimeo/psalm": "^3.11"
    },
    "autoload": {
      "psr-4": {
        "Akukoder\\FortifyTablerAdmin\\": "src"
      }
    },
    "scripts": {
      "psalm": "vendor/bin/psalm",
      "test": "vendor/bin/phpunit --colors=always",
      "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
      "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
      "sort-packages": true
    },
    "extra": {
      "laravel": {
        "providers": [
          "Akukoder\\FortifyTablerAdmin\\FortifyTablerAdminServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
  